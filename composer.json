{
    "name": "chicorycom/installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Assane Sarr",
            "email": "contact@chicorycom.net"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "require-dev": {
    "phpunit/phpunit": "^8.0|^9.3"
    },
    "bin": [
        "bin/chicorycom"
    ],
    "autoload": {
        "psr-4": {
            "Chicorycom\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chicorycom\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
